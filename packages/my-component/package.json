{
  "name": "@wefly/vue-ui",
  "version": "1.0.5",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./es/*": "./es/*.mjs",
    "./es/*.mjs": "./es/*.mjs",
    "./es/*/*": "./es/*/*.mjs",
    "./es/*/": "./es/*/index.mjs",
    "./es/*/*.mjs": "./es/*/*.mjs",
    "./cjs/*": "./cjs/*.js",
    "./cjs/*.js": "./cjs/*.js",
    "./cjs/*/": "./cjs/*/index.js",
    "./cjs/*/*": "./cjs/*/*.js",
    "./cjs/*/*.mjs": "./cjs/*/*.js",
    "./*": "./*"
  },
  "files": [
    "dist",
    "es",
    "cjs",
    "scripts"
  ],
  "scripts": {
    "postinstall": "node ./scripts/postinstall.mjs",
    "clean": "rimraf dist",
    "switch:v3": "vue-demi-switch 3 vue3",
    "switch:v2": "vue-demi-switch 2 vue2",
    "dev": "npm run switch:v3 && cross-env NODE_ENV=development vite",
    "build:v3": "npm run switch:v3 && jiti ./build/component",
    "build:v2": "npm run switch:v2 && jiti ./build/component",
    "build": "npm run clean && npm run build:v3 && npm run build:v2",
    "build:m": "npm run clean && jiti ./build/modules"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "fs-extra": "^10.1.0",
    "vue": "3.2.41",
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/node": "^18.11.2",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/composition-api": "^1.7.1",
    "@vue/runtime-dom": "^3.2.31",
    "esbuild": "0.14.47",
    "fast-glob": "^3.2.12",
    "glob": "^8.0.3",
    "jiti": "^1.16.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "local-pkg": "^0.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-esbuild": "^4.10.1",
    "typescript": "^4.6.4",
    "unplugin-vue2-script-setup": "^0.11.3",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6",
    "vite-plugin-vue2": "^2.0.2",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "^0.40.4",
    "vue2": "npm:vue@2.6.14",
    "vue3": "npm:vue@^3.2.41"
  }
}
